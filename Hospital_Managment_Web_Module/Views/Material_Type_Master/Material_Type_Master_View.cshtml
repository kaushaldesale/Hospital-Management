@model Hospital_Managment_Model_Layer.Master_Model.Material_Type_Information_Model
<html lang="en">
<head>
   
    <script>

        $(function () {
            var availableTags = [
                "ActionScript",
                "AppleScript",
                "Asp",
                "BASIC",
                "C",
                "C++",
                "Clojure",
                "COBOL",
                "ColdFusion",
                "Erlang",
                "Fortran",
                "Groovy",
                "Haskell",
                "Java",
                "JavaScript",
                "Lisp",
                "Perl",
                "PHP",
                "Python",
                "Ruby",
                "Scala",
                "Scheme"
            ];
            $("#tags").autocomplete({
                source: availableTags
            });
        });
    </script>
</head>
<body class="bg-primary">
    @using (Html.BeginForm("SaveOrUpdate", "Material_Type_Master", FormMethod.Post, new { enctype = "multipart/form-data" }))
    {
        @Html.AntiForgeryToken()
        @Html.ValidationSummary(true, "", new { @class = "text-danger" })

        <div class="form-row ">
            <div class="col-md-3">
                <div class="form-group ">
                    <label class="small mb-1 ">Material Type<small style="color:red">*</small></label>
                    @Html.HiddenFor(m => m.material_type_id)
                    @Html.TextBoxFor(m => m.material_type, new { @class = "form-control form-control-sm sm-2 border border-dark disabled", autofocus = "autofocus", tabindex = "2" })
                    @Html.ValidationMessageFor(m => m.material_type, "", new { @class = "text-danger  " })
                </div>
                <div class="form-group ">
                    <label class="small mb-1 ">Material Type<small style="color:red">*</small></label>
                    <input id="tags">
                </div>
            </div>
           
          
        </div>
        <div class="form-group mt-4 mb-0 form-actions">
            <button type="submit" class="btn btn-dark" id="btnsubmit"> Save Record </button>
        </div>
    }

</body>

</html>





