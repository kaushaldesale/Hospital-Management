@model Hospital_Managment_Model_Layer.Master_Model.Item_Information_Model
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<head>
    <script src="https://unpkg.com/sweetalert/dist/sweetalert.min.js"></script>
    @if (TempData["message"] != null)
    {
        <script type="text/javascript">
                    window.onload = function () {
                        var message = '@TempData["message"]';
                 swal("Good job!", message, "success");
                    };
        </script>
    }
    @if (TempData["Error"] != null)
    {
        <script type="text/javascript">
                    window.onload = function () {
                        var message = '@TempData["Error"]';
                 swal("Try Again!", message, "error");
                    };
        </script>
    }
</head>
<html lang="en">

<body>
    <div id="layoutAuthentication">
        <div id="layoutAuthentication_content">
            <main>
                <div class="container-fluid">
                    <div class="row justify-content-center">
                        <div class="col-lg-12">
                            <div class="cord-body mt-1">

                                <!-- Nav tabs -->
                                <ul class="nav nav-tabs">
                                    <li class="nav-item">
                                        <a class="nav-link active" data-toggle="tab" href="#Insert">Insert data</a>
                                    </li>
                                    <li class="nav-item">
                                        <a class="nav-link" data-toggle="tab" href="#Report">Update/Delete</a>
                                    </li>

                                </ul>

                                <!-- Tab panes -->
                                <div class="tab-content">
                                    <div id="Insert" class="container-fluid tab-pane active">
                                        <div class="card-header bg-dark">
                                            <h5 class="font-weight-bolder text-center    text-white">
                                                Item Master
                                             
                                            </h5>
                                        </div>
                                        <div class="card-body card shadow-lg border-0 rounded-lg">
                                            @Html.Partial("~/Views/Item_Master/Item_Master_View.cshtml")
                                        </div>
                                    </div>
                                    <div id="Report" class="container-fluid tab-pane fade">
                                        <div class="card-header bg-dark ">

                                            <h5 class="font-weight-bolder text-center text-white">
                                                Shop Registration Report
                                            </h5>
                                        </div>
                                        @*<div class="card-body card shadow-lg border-0 rounded-lg">
                                            @Html.Action("Trading_Shop_Branch_Report")
                                        </div>*@
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </main>
        </div>
    </div>
</body>

</html>
@section scripts{
    @Scripts.Render("~/bundles/jqueryval")
}
